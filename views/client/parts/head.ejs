<head>
    <meta charset="UTF-8">
    <script>
      // Suppress specific Chrome Extension errors affecting Lighthouse Best Practices
      (function() {
        const originalConsoleError = console.error;
        console.error = function(...args) {
          if (args.length > 0 && typeof args[0] === 'string' && 
             (args[0].includes('chrome-extension://') || args[0].includes('share-modal.js'))) {
            return;
          }
          originalConsoleError.apply(console, args);
        };
      })();
    </script>
    <title><%- title %></title>
    <meta name="description" content="<%= description %>">
    <link rel="canonical" href="<%= canonical %>">
    <!-- Open Graph / Facebook -->
    <meta property="og:title" content="<%- title %>">
    <meta property="og:description" content="<%- description %>">
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%- canonical %>">
    <meta property="og:image" content="<%- og_image %>">
    <!-- Twitter Cards -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%- title %>">
    <meta name="twitter:description" content="<%- description %>">
    <meta name="twitter:image" content="<%- og_image %>">

    <!-- Mobile / Browser -->
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#ffffff">
    
    <!-- Icons -->
    <link rel="icon" href="/images/favicon.png">
    <link rel="apple-touch-icon" href="/client/images/favicon.png">
    <link rel="manifest" href="/manifest.json">

    <!-- Preload LCP Image -->
    <% if (typeof nav_active !== 'undefined' && nav_active === 'home') { %>
    <link rel="preload" as="image" href="/images/header/header-background.webp" type="image/webp" fetchpriority="high">
    <% } %>
    <link rel="preload" as="font" href="/client/assets/fonts/NotoSansArmenian.ttf" type="font/ttf" crossorigin>
    
    <!-- Preload Critical CSS -->
    <link rel="preload" href="/client/dist/css/coreui.min.css" as="style">
    <link rel="preload" href="/client/dist/css/main.css" as="style">

    <!-- vendor -->
    <link rel="stylesheet" href="/client/vendors/css/all.min.css" media="print" onload="this.media='all'" />
    <noscript><link rel="stylesheet" href="/client/vendors/css/all.min.css" /></noscript>

    <% if (typeof nav_active !== 'undefined' && nav_active === 'home') { %>
    <link rel="stylesheet" href="/client/vendors/css/lightgallery.min.css" media="print" onload="this.media='all'">
    <noscript><link rel="stylesheet" href="/client/vendors/css/lightgallery.min.css"></noscript>

    <link rel="stylesheet" href="/client/vendors/css/swiper-bundle.min.css" media="print" onload="this.media='all'">
    <noscript><link rel="stylesheet" href="/client/vendors/css/swiper-bundle.min.css"></noscript>
    <% } %>

    <!-- CoreUI CSS -->
    <link rel="stylesheet" href="/client/dist/css/coreui.min.css">

    <% if (typeof nav_active !== 'undefined' && ['profile', 'options', 'info'].includes(nav_active)) { %>
    <link rel="stylesheet" href="/client/vendors/css/cropper.css" media="print" onload="this.media='all'">
    <noscript><link rel="stylesheet" href="/client/vendors/css/cropper.css"></noscript>
    <% } %>

    <!-- Main CSS -->
    <link rel="stylesheet" href="/client/dist/css/main.css">
</head>
